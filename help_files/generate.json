{
    "name": "gen",
    "about": "Generates a template in a specific folder ",
    "parents": [
        "tp"
    ],
    "args": {
        "inputs": [
            {
                "name": "temp-name",
                "help": "Sets the template name",
                "required": true
            },
            {
                "name": "output-folder",
                "help": "Sets the output folder",
                "default_value": "./"
            },
            {
                "name": "remote-template-url",
                "help": "Sets the remote template url when generating remote template"
            }
        ],
        "flags": [
            {
                "long": "file",
                "short": "f",
                "help": "Generates from template file"
            },
            {
                "long": "remote",
                "help": "Generates from remote template"
            }
        ],
        "options": [
            {
                "long": "key",
                "value_name": "authorization-key",
                "help": "Sets the authorization key when getting remote template"
            }
        ]
    }
}